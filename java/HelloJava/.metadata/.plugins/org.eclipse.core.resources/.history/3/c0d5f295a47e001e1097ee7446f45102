
public class ScoreV1 {

	public static void main(String[] args) {
		// 성적표
		String name = "둘리";
		int kor = 90;
		int eng = 89;
		int math = 75;
		
		// 총점, 평균, 학점 구해서 출력
		int total = kor + eng + math;
		double avg = total / 3;
		System.out.println("총점" + total);
		System.out.println("평균" + avg);
		

	}

}
